




{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/server/(.*)",
        "dest": "/server/$1"
      },
      {
        "src": "/client/(.*)",
        "dest": "/client/build/$1"
      }
    ]
  }